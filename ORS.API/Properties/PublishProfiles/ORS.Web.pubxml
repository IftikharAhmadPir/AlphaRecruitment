<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://alpharecruiters.org:7521</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>True</ExcludeApp_Data>
    <MSDeployServiceURL>alpharecruiters.org</MSDeployServiceURL>
    <DeployIisAppPath>ORS.API</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>False</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>administrator</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="ORSDB" Order="1" Enabled="True">
          <Destination Path="Data Source=.\sqlexpress;Initial Catalog=ORSDB;User ID=sa;Password=someone@786" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="ORS.API.AuthContext, ORS.API" MigrationConfiguration="ORS.API.Migrations.Configuration, ORS.API" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <PrecompileBeforePublish>True</PrecompileBeforePublish>
    <EnableUpdateable>True</EnableUpdateable>
    <DebugSymbols>False</DebugSymbols>
    <WDPMergeOption>DonotMerge</WDPMergeOption>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)ORSDB-Web.config Connection String">
      <ParameterValue>Data Source=.\sqlexpress;Initial Catalog=ORSDB;User ID=sa;Password=someone@786</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>